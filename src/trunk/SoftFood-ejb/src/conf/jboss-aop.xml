<aop>
  
    <aspect name="logging" class="it.softfood.aspect.LoggingAspect"/>

    
    
    <bind pointcut="execution(* it.softfood.session.ordinazione.OrdinazioneSessionBean->inserisciOrdinazione(it.softfood.entity.Ordinazione))">
        <advice name="logOrder" aspect="logging"/>
    </bind>
    
    
</aop>

